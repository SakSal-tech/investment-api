        { "name": "Get Clients by Active Status", "request": { "method": "GET", "url": { "raw": "http://localhost:8080/api/clients/search/active?isActive={{isActive}}", "host": [ "http://localhost:8080" ], "path": [ "api", "clients", "search", "active" ], "query": [ { "key": "isActive", "value": "{{isActive}}" } ] } } },
        { "name": "Get Total Client Count", "request": { "method": "GET", "url": { "raw": "http://localhost:8080/api/clients/count", "host": [ "http://localhost:8080" ], "path": [ "api", "clients", "count" ] } } },
        { "name": "Get Active Client Count", "request": { "method": "GET", "url": { "raw": "http://localhost:8080/api/clients/count/active", "host": [ "http://localhost:8080" ], "path": [ "api", "clients", "count", "active" ] } } },
        { "name": "Get Latest Clients (limit)", "request": { "method": "GET", "url": { "raw": "http://localhost:8080/api/clients/latest?limit={{limit}}", "host": [ "http://localhost:8080" ], "path": [ "api", "clients", "latest" ], "query": [ { "key": "limit", "value": "{{limit}}" } ] } } },
        { "name": "Create Client", "request": { "method": "POST", "url": { "raw": "http://localhost:8080/api/clients", "host": [ "http://localhost:8080" ], "path": [ "api", "clients" ] }, "body": { "mode": "raw", "raw": "{\n  \"name\": \"{{name}}\",\n  \"email\": \"{{email}}\"\n}" } } },
        { "name": "Update Client", "request": { "method": "PUT", "url": { "raw": "http://localhost:8080/api/clients/{{clientId}}", "host": [ "http://localhost:8080" ], "path": [ "api", "clients", "{{clientId}}" ] }, "body": { "mode": "raw", "raw": "{\n  \"name\": \"{{name}}\"\n}" } } },
        { "name": "Delete Client", "request": { "method": "DELETE", "url": { "raw": "http://localhost:8080/api/clients/{{clientId}}", "host": [ "http://localhost:8080" ], "path": [ "api", "clients", "{{clientId}}" ] } } }
      ]
    },
    {
