  "item": [
    {
      "name": "Clients",
      "item": [
        { "name": "Get All Clients", "request": { "method": "GET", "url": { "raw": "http://localhost:8080/api/clients", "host": [ "http://localhost:8080" ], "path": [ "api", "clients" ] } } },
        { "name": "Get Client by ID", "request": { "method": "GET", "url": { "raw": "http://localhost:8080/api/clients/{{clientId}}", "host": [ "http://localhost:8080" ], "path": [ "api", "clients", "{{clientId}}" ] } } },
        { "name": "Get Clients by Email Server", "request": { "method": "GET", "url": { "raw": "http://localhost:8080/api/clients/search/email?emailServer={{emailServer}}", "host": [ "http://localhost:8080" ], "path": [ "api", "clients", "search", "email" ], "query": [ { "key": "emailServer", "value": "{{emailServer}}" } ] } } },
        { "name": "Get Clients by Postcode", "request": { "method": "GET", "url": { "raw": "http://localhost:8080/api/clients/search/postcode?postcode={{postcode}}", "host": [ "http://localhost:8080" ], "path": [ "api", "clients", "search", "postcode" ], "query": [ { "key": "postcode", "value": "{{postcode}}" } ] } } },
