        { "name": "Find User by Reset Token", "request": { "method": "GET", "url": { "raw": "http://localhost:8080/api/users/reset-token/{{token}}", "host": [ "http://localhost:8080" ], "path": [ "api", "users", "reset-token", "{{token}}" ] } } },
        { "name": "Reset User Password", "request": { "method": "POST", "url": { "raw": "http://localhost:8080/api/users/reset-password?username={{username}}&newPassword={{newPassword}}&resetToken={{resetToken}}", "host": [ "http://localhost:8080" ], "path": [ "api", "users", "reset-password" ], "query": [ { "key": "username", "value": "{{username}}" }, { "key": "newPassword", "value": "{{newPassword}}" }, { "key": "resetToken", "value": "{{resetToken}}" } ] } } },
        { "name": "Create User", "request": { "method": "POST", "url": { "raw": "http://localhost:8080/api/users", "host": [ "http://localhost:8080" ], "path": [ "api", "users" ] }, "body": { "mode": "raw", "raw": "{\n  \"username\": \"{{username}}\",\n  \"email\": \"{{email}}\"\n}" } } },
        { "name": "Update User", "request": { "method": "PUT", "url": { "raw": "http://localhost:8080/api/users/{{username}}", "host": [ "http://localhost:8080" ], "path": [ "api", "users", "{{username}}" ] }, "body": { "mode": "raw", "raw": "{\n  \"email\": \"{{email}}\"\n}" } } },
        { "name": "Delete User", "request": { "method": "DELETE", "url": { "raw": "http://localhost:8080/api/users/{{username}}", "host": [ "http://localhost:8080" ], "path": [ "api", "users", "{{username}}" ] } } }
      ]
    },
